#!/bin/bash
# Properly manipulates the FlatZinc output stream for a given CSP.

OUT=$1

while read line
do
  # On std output print the solution.
  echo $line | grep " = \|---------"
  # On output file we need just the solver outcome.
  echo $line | grep "==========\|=====UNSATISFIABLE=====\|---------" >> $OUT
done
